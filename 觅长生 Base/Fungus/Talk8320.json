        },
        {
          "ItemID": 290,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "NPC2",
          "StoryText": "嗯？炼妖散炼成了？听总坛的人说只是小有进展吧。",
