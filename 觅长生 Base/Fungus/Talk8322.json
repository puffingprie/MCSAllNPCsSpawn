        },
        {
          "ItemID": 410,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "NPCzhongyan",
          "StoryText": "我此行匆忙身无长物，这几枚血菩提还请{shidi}收下。",
