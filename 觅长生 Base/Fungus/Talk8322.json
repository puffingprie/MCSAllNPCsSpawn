        },
        {
          "ItemID": 408,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "NPCyanhe",
          "StoryText": "{FirstName}长老，此番凶险多亏你了。",
          "Description": "",
