        },
        {
          "ItemID": 429,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "NPCzhongyan",
          "StoryText": "多谢，{shidi}出手相救。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 430,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "NPCzhongyan",
          "StoryText": "钟师兄怎么会被古神教的宵小困于此地。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 431,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "NPCzhongyan",
          "StoryText": "我之前得知此地有人搜捕豹麟兽，但是抵达此地时却是中伏了。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 432,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "NPCzhongyan",
          "StoryText": "说来也是我大意，险些着了他们的道。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 433,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "NPCzhongyan",
          "StoryText": "不必了，此阵一破我就通知傻鸟他们前来接我了。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 428,
          "CmdType": "Fungus.Else"
        },
        {
          "ItemID": 434,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "NPCzhongyan",
          "StoryText": "多谢，道友出手相救。在下离火门绝焰堂大长老钟焰，敢问阁下姓名？",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 435,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "NPCzhongyan",
          "StoryText": "{FirstName}{LastName}。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 436,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "NPCzhongyan",
          "StoryText": "原来是{FirstName}道友，我虽久居麟庙却也对你早有耳闻了。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 437,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "NPCzhongyan",
          "StoryText": "客气了，道友为何会被这古神教的妖人在此伏击？",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 439,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "NPCzhongyan",
          "StoryText": "我之前有事来此处理，但是抵达此地时却是中伏了。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 440,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "NPCzhongyan",
          "StoryText": "（看来这离火门内部也是....）",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 441,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "NPCzhongyan",
          "StoryText": "道友身受重伤可是需要我送你回去？",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 442,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "NPCzhongyan",
          "StoryText": "不必了，此阵一破我就通知同门前来接我了。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
