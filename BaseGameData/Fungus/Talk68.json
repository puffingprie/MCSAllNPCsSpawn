        {
          "ItemID": 248,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "avater2",
          "StoryText": "此事便交给你了。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 249,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "Avatar",
          "StoryText": "是，大长老。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 250,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "Avatar",
          "StoryText": "望风殿前，一名身材高大的黑袍男子似乎正在交代着什么，令你意外的是，他并没有佩戴风雨楼的面具，这也让你得以注意到他的样貌。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 251,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "Avatar",
          "StoryText": "大长老...此人莫非便是琚运琦？",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 252,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "Avatar",
          "StoryText": "正当你思索之际，那名男子已经转身向着你的方向大步流星的走来，你决定...",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
