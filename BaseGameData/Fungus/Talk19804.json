        },
        {
          "ItemID": 408,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "NPC1",
          "StoryText": "我们都曾以为那位大护法是为了研发炼妖散的志同道合之辈，也是我玄古门中兴之人，但是看错了。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 409,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "NPC1",
