      "Position": "(-2424.038,-1018.337)",
      "Commands": [
        {
          "ItemID": 374,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "",
          "StoryText": "在你和这怪物纠缠的时候，玄古门弟子已经被斩杀殆尽了，仅剩那名元婴修士。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 375,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "NPCxuangu2",
          "StoryText": "还想着自爆元婴？",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 441,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "",
          "StoryText": "一击惊魂咒命中他，他愣神间也被阵法斩杀，尸体瞬间被抽干。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 458,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "NPCxuangu2",
          "StoryText": "这位小友我还有事，剑你帮我保管好。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 442,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "NPCxuangu2",
          "StoryText": "说罢玄骨携带着一团精血破开空间，转身离开。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 444,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "",
          "StoryText": "此时被你击败的掌门也恢复了理智。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 445,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "MonsterID",
          "StoryText": "咳~咳~~~道友，可认识岚诺？",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 446,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "MonsterID",
          "StoryText": "你就是他师傅吧，他本来是托我救你的。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 447,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "MonsterID",
          "StoryText": "我？我不配，倒是道友见到他记得告诉他不要报仇保住宗门传承要紧。我们斗不过古神教的。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 448,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "",
          "StoryText": "说罢他身体失去支撑而倒下，气息开始消散，肉身也逐渐崩解。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 449,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "",
          "StoryText": "这就是炼妖散么？把自己变成这不人不妖的怪物，最后落个形神俱灭。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 450,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 2,
          "AvatarBindKey": "",
          "StoryText": "长生难求，总有人想另辟蹊径啊。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 457,
          "CmdType": "Fungus.SetStaticValue"
        },
        {
          "ItemID": 451,
          "CmdType": "Fungus.GetStaticValue"
        },
        {
          "ItemID": 452,
          "CmdType": "Fungus.If",
          "Condition": "TempValue == 1"
        },
        {
          "ItemID": 454,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "",
          "StoryText": "算了，先去看看轩岚诺吧。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 453,
          "CmdType": "Fungus.Else"
        },
        {
          "ItemID": 455,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "",
          "StoryText": "算了，先出去吧。",
          "Description": "",
          "AvatarIDSetType": "Normal"
