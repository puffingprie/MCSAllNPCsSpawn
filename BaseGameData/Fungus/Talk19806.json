        },
        {
          "ItemID": 483,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "NPC1",
          "StoryText": "唉...",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 484,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "",
          "StoryText": "阵法运转，空间一阵波动此人和阵中的精血都不见了。",
          "Description": "",
